      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "support.class",
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Named Type",
